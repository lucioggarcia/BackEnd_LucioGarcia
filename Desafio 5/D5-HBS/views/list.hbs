<a href="/">IR A CARGA DE PRODUCTOS</a>
<h2>Lista de Productos</h2>
{{#if products.length }}
  <table>
    <tr>
      <th>Titulo</th>
      <th>Precio</th>
      <th>Foto</th>
    </tr>
    {{#each products}}
      <tr>
        <td>{{title}}</td>
        <td>{{price}}</td>
        <td><img src="{{thumbnail}}" alt=""></td>
      </tr>
    {{/each}}
  </table>
{{else}}
  <h3>No hay datos</h3>
{{/if}}